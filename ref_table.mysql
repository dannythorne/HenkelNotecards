
use HenkelNotecards;

# Create the References table.
# Note that "references" is a reserved word in MySQL,
# so an alternative like "Refs" is required.
drop table if exists Refs;
create table
Refs( WorkId int
    , NotecardId int
    , coords varchar(32)
    , primary key (WorkId, NotecardId) );

